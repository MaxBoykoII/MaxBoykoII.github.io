    "bower": "^1.7.9",
    "browserify": "^13.1.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-inject": "^4.1.0",
    "gulp-load-plugins": "^1.3.0",
    "gulp-sass": "^2.3.2",
